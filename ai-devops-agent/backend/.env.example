# .env.example â€” copy to .env and fill in values

# GitHub personal access token (repo + workflow scopes required)
GITHUB_TOKEN=""

# LLM Provider: "openai" | "gemini" | "none"
LLM_PROVIDER=gemini

# Gemini (if LLM_PROVIDER=gemini)
GEMINI_API_KEY=""
GEMINI_MODEL=gemini-2.5-flash

# Agent settings
MAX_ITERATIONS=5
TEST_TIMEOUT=120
LINT_TIMEOUT=60

# Docker sandboxing
DOCKER_ENABLED=false
DOCKER_IMAGE=python:3.11-slim

# API server
API_HOST=0.0.0.0
API_PORT=8000
